filters:
  and:
    - file.hasTag("reference/concept")
formulas:
  Concept: link(file, aliases[0])
  Group: aliases[0][0]
views:
  - type: list
    name: By Letter
    groupBy:
      property: formula.Group
      direction: ASC
    order:
      - formula.Concept
    sort:
      - property: formula.Concept
        direction: ASC
